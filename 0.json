{
   "Version": "2012-10-17",
   "Statement": [
       {
           "Sid": "AllowS3ActionsInUsEast1",
           "Effect": "Allow",
           "Action": "s3:*",
           "Resource": "*",
           "Condition": {
               "StringEquals": {
                   "aws:RequestedRegion": "us-east-1"
               }
           }
       }
   ]
}
